{
  "name": "moomin",
  "version": "0.0.1",
  "description": "My first app",
  "engines": {
    "node": ">=14.17.0 <16.3.0",
    "npm": "~6.14.11",
    "go": "1.16"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "contributors": [
    "stephen@epyo.eu"
  ],
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest",
    "prepare": "husky install",
    "release": "standard-version",
    "prerelease": "standard-version -- --prerelease",
    "fix": "standard-version -- --release-as minor",
    "package:update": "npx npm-check-updates -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephendltg/moomin-cyan.git"
  },
  "keywords": [
    "golang",
    "webview"
  ],
  "author": "stephendltg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stephendltg/moomin-cyan/issues"
  },
  "homepage": "https://github.com/stephendltg/moomin-cyan#readme",
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "core-js": "^3.15.1",
    "nuxt": "^2.15.7",
     "@stephendltg/e-bus": "^1.0.8",
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "husky": "^7.0.1",
    "standard-version": "^9.3.1",
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "jest": "^27.0.5",
    "lint-staged": "^10.5.4",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "vue-jest": "^3.0.4"
  }
}
